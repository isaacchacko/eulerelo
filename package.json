{
  "name": "eulerelo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"nodemon ./src/socket-server/server.js\" \"docker compose -f ./prisma/docker-compose.yml up\"",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "start:socket": "node ./src/socket-server/server.js",
    "lint": "next lint",
    "postinstall": "prisma generate --no-engine"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@geist-ui/icons": "^1.0.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.11.1",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cookies-next": "^4.1.1",
    "geist": "^1.4.2",
    "katex": "^0.16.22",
    "mathjs": "^14.4.0",
    "next": "^15.2.3",
    "next-auth": "^4.24.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-katex": "^3.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.17.27",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "prisma": "^6.11.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  }
}
